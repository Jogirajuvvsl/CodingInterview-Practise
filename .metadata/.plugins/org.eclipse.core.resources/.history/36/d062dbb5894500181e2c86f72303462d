
public class RotateArray {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int numbers[]= {1,3,4,5};
	    int noOfRotataions=4;
	    int effectiveRotations=numbers.length%noOfRotataions;
	    if(effectiveRotations>0)
	    {
	    	 int temp[]=new int[effectiveRotations];
	    	 for(int i=0;i<effectiveRotations;i++)
	    	  temp[i]=numbers[i];
	    int j=0,i=0,k=effectiveRotations;
	    	
	    for(i=effectiveRotations;i<numbers.length;i++) 
	     numbers[j++]=numbers[i];
	    for(j=0;j<temp.length;j++)
	    numbers[i++]=temp[j];
	    for(int ele:numbers)
	    	System.out.println(ele);
	    	
	    }
	    else
	    {
	    	
	    	for(int ele:numbers)
	    		System.out.println(ele);
	    	
	    }
		
	}

}
